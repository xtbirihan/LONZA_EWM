<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">                                                                                                                                          
	<VBox height="17rem">                                                                                                                                                                                                                                         
		<HBox height="3rem"/>                                                                                                                                                                                                                                        
		<FlexBox direction="Column" width="100%" class="sapUiSmallMargin sapUiLargeMarginTop sapUiSmallMarginBottom" justifyContent="Center">                                                                                                                        
			<Button type="Unstyled" width="90%" enabled="false" class="arrowButton" press="onConfirmPressed" id="btn_confirm" text="{i18n>Confirm}"/>                                                                                                                   
		</FlexBox>                                                                                                                                                                                                                                                   
		<FlexBox direction="Column" width="100%" class="sapUiSmallMargin" justifyContent="Center">                                                                                                                                                                   
			<Button type="Unstyled" class="putbackBtn" id="btn_confirm_zero" width="90%" text="{i18n>ConfirmZeroQty}"                                                                                                                                                   
					press="onConfirmZeroQty" enabled="false">                                                                                                                                                                                                                 
			</Button>                                                                                                                                                                                                                                                   
		</FlexBox>                                                                                                                                                                                                                                                   
</VBox>                                                                                                                                                                                                                                                        
</core:FragmentDefinition>                                                                                                                                                                                                                                     