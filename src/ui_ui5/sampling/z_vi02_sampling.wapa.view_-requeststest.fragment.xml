 <core:FragmentDefinition                                                                                                                                                                                                                                      
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core">                                                                                                                                                                                                                                  
    <Dialog afterClose="loginAfterClose" id="testDialog" title="Login">                                                                                                                                                                                        
        <content>                                                                                                                                                                                                                                              
            <VBox class="sapUiSmallMargin" justifyContent="Center">                                                                                                                                                                                            
                <HBox justifyContent="Center">                                                                                                                                                                                                                 
                    <Button text="Close" class="sapUiSmallMarginBeginEnd" press="onCloseTesting" />                                                                                                                                                            
                </HBox>                                                                                                                                                                                                                                        
                <HBox justifyContent="Center">                                                                                                                                                                                                                 
                    <Label text="Warehouse Number" labelFor="lgnumInput" />                                                                                                                                                                                    
                    <Input                                                                                                                                                                                                                                     
                        id="lgnumInput"                                                                                                                                                                                                                        
                        placeholder="Warehouse Number"/>                                                                                                                                                                                                       
                </HBox>                                                                                                                                                                                                                                        
                <HBox class="sapUiSmallMarginTop" justifyContent="Center">                                                                                                                                                                                     
                    <Label text="Workcenter" labelFor="workcenterInput" />                                                                                                                                                                                     
                    <Input                                                                                                                                                                                                                                     
                        id="workcenterInput"                                                                                                                                                                                                                   
                        placeholder="Workcenter"/>                                                                                                                                                                                                             
                </HBox>                                                                                                                                                                                                                                        
                <HBox class="sapUiSmallMarginTop" justifyContent="Center">                                                                                                                                                                                     
                    <Label text="Handling Unit" labelFor="HUNum" />                                                                                                                                                                                            
                    <Input                                                                                                                                                                                                                                     
                        id="HUNum"                                                                                                                                                                                                                             
                        placeholder="Enter Handling Unit"/>                                                                                                                                                                                                    
                </HBox>                                                                                                                                                                                                                                        
                <HBox class="sapUiSmallMarginTop" justifyContent="Center">                                                                                                                                                                                     
                    <Label text="MSEHL" labelFor="msehlInput" />                                                                                                                                                                                               
                    <Input                                                                                                                                                                                                                                     
                        id="msehlInput"                                                                                                                                                                                                                        
                        placeholder="MSEHL"/>                                                                                                                                                                                                                  
                </HBox>                                                                                                                                                                                                                                        
                <HBox class="sapUiSmallMarginTop" justifyContent="Center">                                                                                                                                                                                     
                    <Label text="MSEHI" labelFor="msehiInput" />                                                                                                                                                                                               
                    <Input                                                                                                                                                                                                                                     
                        id="msehiInput"                                                                                                                                                                                                                        
                        placeholder="MSEHI"/>                                                                                                                                                                                                                  
                </HBox>                                                                                                                                                                                                                                        
                <HBox class="sapUiSmallMarginTop" justifyContent="Center">                                                                                                                                                                                     
                    <Label text="QTY" labelFor="qtyInput" />                                                                                                                                                                                                   
                    <Input                                                                                                                                                                                                                                     
                        id="qtyInput"                                                                                                                                                                                                                          
                        placeholder="QTY"/>                                                                                                                                                                                                                    
                </HBox>                                                                                                                                                                                                                                        
            </VBox>                                                                                                                                                                                                                                            
            <Button text="HUHeaderSet" class="sapUiSmallMarginBeginEnd" press="onHUHeaderPressed"/>                                                                                                                                                            
            <Button text="GetUserDefaultWarehouse" class="sapUiSmallMarginBeginEnd" press="onGetLgnumPressed" />                                                                                                                                               
            <Button text="Login" class="sapUiSmallMarginBeginEnd" press="onTestLoginPressed" />                                                                                                                                                                
            <Button text="Logoff" class="sapUiSmallMarginBeginEnd" press="onTestLogoffPressed" />                                                                                                                                                              
            <Button text="VH_Workstation" class="sapUiSmallMarginBeginEnd" press="onGetWCsPressed" />                                                                                                                                                          
            <Button text="VH_WarehouseNumber" class="sapUiSmallMarginBeginEnd" press="onGetLgnumsPressed" />                                                                                                                                                   
            <Button text="VH_UnitofMeasure" class="sapUiSmallMarginBeginEnd" press="onGetUoMsPressed" />                                                                                                                                                       
            <Button text="PostQuantity" class="sapUiSmallMarginBeginEnd" press="onPostRequest" />                                                                                                                                                              
        </content>                                                                                                                                                                                                                                             
    </Dialog>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>                                                                                                                                                                                                                                     