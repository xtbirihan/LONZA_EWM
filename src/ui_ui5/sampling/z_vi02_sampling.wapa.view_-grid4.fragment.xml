<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">                                                                                                                                          
	<VBox height="17rem">                                                                                                                                                                                                                                         
		<VBox>                                                                                                                                                                                                                                                       
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>topHU}" class="customTextTiny"/>                                                                                                                                                                                                        
					<Text text="{selectedRow>/HigherHuident}" class="customTextTiny"/>                                                                                                                                                                                        
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>srcHU}" class="customTextTiny"/>                                                                                                                                                                                                        
					<Text text="{  path: 'selectedRow>/Vhuident',                                                                                                                                                                                                             
								formatter: '.formatter.checkIfProvided'                                                                                                                                                                                                                
							 }" class="customTextTiny"/>                                                                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>srcPmatHUType}" class="customTextTiny"/>                                                                                                                                                                                                
					<Text id="pmatTxt" text="{  parts: ['selectedRow>/Vpmat',                                                                                                                                                                                                 
														'selectedRow>/Vletyp'],                                                                                                                                                                                                                          
								formatter: '.formatter.checkIfSrcHuProvided'                                                                                                                                                                                                           
							 }" class="customTextTiny"/>                                                                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>Matnr}" class="customTextTiny"/>                                                                                                                                                                                                        
					<Text text="{selectedRow>/NavToHUItem/Matnr}" class="customTextTiny"/>                                                                                                                                                                                    
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>Descr}" wrapping="false" class="customTextTiny"/>                                                                                                                                                                                       
					<Text text="{  path: 'selectedRow>/NavToHUItem/Maktx',                                                                                                                                                                                                    
								formatter: '.formatter.longTextFormatter'                                                                                                                                                                                                              
							 }" wrapping="false" class="customTextTiny"/>                                                                                                                                                                                                           
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>Batch}" class="customTextTiny"/>                                                                                                                                                                                                        
					<Text text="{selectedRow>/NavToHUItem/Charg}" class="customTextTiny"/>                                                                                                                                                                                    
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>StType}" class="customTextTiny"/>                                                                                                                                                                                                       
					<Text text="{  path: 'selectedRow>/NavToHUItem/Cat',                                                                                                                                                                                                      
								formatter: '.formatter.checkIfProvided'                                                                                                                                                                                                                
							 }" class="customTextTiny"/>                                                                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>PartyDisp}" class="customTextTiny"/>                                                                                                                                                                                                    
					<Text text="{  path: 'selectedRow>/NavToHUItem/Entitled',                                                                                                                                                                                                 
								formatter: '.formatter.checkIfProvided'                                                                                                                                                                                                                
							 }" class="customTextTiny"/>                                                                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
			<FlexBox justifyContent="SpaceBetween">                                                                                                                                                                                                                     
				<items>                                                                                                                                                                                                                                                    
					<Text text="{i18n>HandlIndic}" class="customTextTiny"/>                                                                                                                                                                                                   
					<Text text="{  parts: ['selectedRow>/NavToHUItem/Hndlcode',                                                                                                                                                                                               
										   'selectedRow>/NavToHUItem/HndlcodeT'],                                                                                                                                                                                                            
								formatter: '.formatter.checkIfHndlTypeProvided'                                                                                                                                                                                                        
							 }" class="customTextTiny"/>                                                                                                                                                                                                                            
				</items>                                                                                                                                                                                                                                                   
			</FlexBox>                                                                                                                                                                                                                                                  
		</VBox>                                                                                                                                                                                                                                                      
	</VBox>                                                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     