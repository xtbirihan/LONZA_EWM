<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table">                                                                                                               
	<VBox height="23rem">                                                                                                                                                                                                                                         
		<VBox alignContent="Center">                                                                                                                                                                                                                                 
		<FlexBox direction="Column" width="100%" class="sapUiSmallMargin" justifyContent="Center">                                                                                                                                                                   
			<Button type="Unstyled" class="putbackBtn sapUiBigMarginBottom" width="90%" text="{i18n>PutbackRemaining}"                                                                                                                                                  
					id="btn_putback" press="onPbRemainingPress" enabled="false">                                                                                                                                                                                              
					<layoutData>                                                                                                                                                                                                                                              
						<FlexItemData growFactor="1" />                                                                                                                                                                                                                          
					</layoutData>                                                                                                                                                                                                                                             
			</Button>                                                                                                                                                                                                                                                   
		</FlexBox>                                                                                                                                                                                                                                                   
		<FlexBox direction="Column" width="100%" class="sapUiSmallMargin" justifyContent="Center">                                                                                                                                                                   
			<Button type="Unstyled" class="putbackBtn sapUiBigMarginTop" width="90%" text="{i18n>NoPutbackLeft}"                                                                                                                                                        
					id="btn_no_putback" press="onNoPutbackLeftPress" enabled="false">                                                                                                                                                                                         
			</Button>                                                                                                                                                                                                                                                   
		</FlexBox>                                                                                                                                                                                                                                                   
		</VBox>                                                                                                                                                                                                                                                      
	</VBox>                                                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     